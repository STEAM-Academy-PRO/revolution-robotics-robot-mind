{
  "version": "0.2.0",
  "configurations": [
    {
      "preLaunchTask": "prepare debug binary",
      "type": "probe-rs-debug",
      "flashingConfig": {
        "haltAfterReset": true,
        "flashingEnabled": true,
      },
      "request": "launch",
      "name": "probe_rs run",
      "chip": "atsamd51p19a",
      "coreConfigs": [
        {
          "svdFile": "${workspaceRoot}/../mcu-common/Atmel/ATSAMD51P19A.svd",
          "programBinary": "${workspaceRoot}/Build/Debug/mcu-firmware/rrrc_samd51.elf",
          "rttEnabled": true,
          "rttChannelFormats": [
            {
              "channelNumber": 0,
              "dataFormat": "String",
              "showLocation": false,
              "showTimestamps": false
            }
          ]
        }
      ]
    },
    {
      "type": "probe-rs-debug",
      "request": "attach",
      "name": "probe_rs attach",
      "server": "127.0.0.1:1234",
      "chip": "atsamd51p19a",
      "coreConfigs": [
        {
          "svdFile": "${workspaceRoot}/../mcu-common/Atmel/ATSAMD51P19A.svd",
          "programBinary": "${workspaceRoot}/Build/Debug/mcu-firmware/rrrc_samd51.elf",
          "rttEnabled": true,
          "rttChannelFormats": [
            {
              "channelNumber": 0,
              "dataFormat": "String",
              "showLocation": false,
              "showTimestamps": false
            }
          ]
        }
      ]
    }
  ]
}